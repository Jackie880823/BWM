<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape >
            <solid android:color="@color/translucence_color" />
            <stroke android:color="@android:color/white" android:width="@dimen/_1dp" android:dashGap="@dimen/_3dp" android:dashWidth="@dimen/_3dp"/>
            <corners android:radius="@dimen/_10dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/transparent_color" />
            <stroke android:color="@android:color/white" android:width="@dimen/_1dp"/>
            <corners android:radius="@dimen/_10dp"/>
        </shape>
    </item>
</selector>