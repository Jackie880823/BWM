<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff"
    tools:context="com.madx.bwm.ui.PathRelationshipActivity">

        <include
            android:id="@+id/fragment_main_top"
            layout="@layout/title_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true" />




        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/ll_relationship"
                        android:background="@drawable/bg_white_gray"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                    <TextView
                                        android:id="@+id/tv"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/text_relationship"
                                        android:layout_alignParentLeft="true"
                                        android:padding="@dimen/default_content_padding"
                                        android:textSize="@dimen/text_large_size"/>

                                    <TextView
                                        android:id="@+id/tv_relationship"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_toLeftOf="@+id/iv"
                                        android:padding="@dimen/default_content_padding"
                                        android:paddingTop="@dimen/default_content_padding"
                                        android:paddingBottom="@dimen/default_content_padding"
                                        android:textSize="@dimen/text_large_size"
                                        android:singleLine="true"
                                        android:text="" />
                                    <ImageView
                                        android:id="@+id/iv"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/profile_right"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerInParent="true"
                                        android:layout_marginRight="@dimen/default_view_margin"/>
                            </RelativeLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/default_split_line"/>

                    </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_main"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginLeft="32dp"
                            android:layout_marginTop="16dp">
                                <com.madx.bwm.widget.CircularNetworkImage
                                    android:id="@+id/cni_main"
                                    android:layout_width="80dp"
                                    android:layout_height="80dp"
                                    app:border="false"
                                    app:shadow="false"
                                    app:border_width="0dp"
                                    android:src="@drawable/network_image_default"/>
                                <TextView
                                    android:id="@+id/tv_main"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="16dp"
                                    android:text="@string/text_me"
                                    android:singleLine="true"
                                    android:textSize="@dimen/text_large_size"
                                    android:textColor="@android:color/black"
                                    android:gravity="center_vertical"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_1"
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="72dp"
                                    android:orientation="horizontal">
                                        <View
                                            android:layout_width="2dp"
                                            android:layout_height="50dp"
                                            android:background="@color/tab_color_press4"/>
                                        <View
                                            android:layout_width="70dp"
                                            android:layout_height="2dp"
                                            android:background="@color/tab_color_press4"
                                            android:layout_gravity="center_vertical"
                                        />
                                        <TextView
                                            android:id="@+id/tv_relationship1"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_vertical"
                                            android:text=""
                                            android:textSize="@dimen/text_medium_size"
                                            android:background="@drawable/ll_round_green"
                                            android:padding="@dimen/default_view_margin"/>
                                </LinearLayout>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:layout_marginLeft="32dp">
                                        <com.madx.bwm.widget.CircularNetworkImage
                                            android:id="@+id/cni1"
                                            android:layout_width="80dp"
                                            android:layout_height="80dp"
                                            app:border="false"
                                            app:shadow="false"
                                            app:border_width="0dp"
                                            android:src="@drawable/network_image_default"/>
                                        <TextView
                                            android:id="@+id/tv_name1"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_marginLeft="16dp"
                                            android:text=""
                                            android:singleLine="true"
                                            android:textSize="@dimen/text_large_size"
                                            android:textColor="@android:color/black"
                                            android:gravity="center_vertical"/>
                                </LinearLayout>
                        </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_2"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="72dp"
                            android:orientation="horizontal">
                                <View
                                    android:layout_width="2dp"
                                    android:layout_height="50dp"
                                    android:background="@color/tab_color_press4"/>
                                <View
                                    android:layout_width="70dp"
                                    android:layout_height="2dp"
                                    android:background="@color/tab_color_press4"
                                    android:layout_gravity="center_vertical"
                                    />
                                <TextView
                                    android:id="@+id/tv_relationship2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:text=""
                                    android:textSize="@dimen/text_medium_size"
                                    android:background="@drawable/ll_round_green"
                                    android:padding="@dimen/default_view_margin"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginLeft="32dp">
                                <com.madx.bwm.widget.CircularNetworkImage
                                    android:id="@+id/cni2"
                                    android:layout_width="80dp"
                                    android:layout_height="80dp"
                                    app:border="false"
                                    app:shadow="false"
                                    app:border_width="0dp"
                                    android:src="@drawable/network_image_default"/>
                                <TextView
                                    android:id="@+id/tv_name2"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="16dp"
                                    android:text=""
                                    android:singleLine="true"
                                    android:textSize="@dimen/text_large_size"
                                    android:textColor="@android:color/black"
                                    android:gravity="center_vertical"/>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_3"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="72dp"
                                android:orientation="horizontal">
                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="50dp"
                                        android:background="@color/tab_color_press4"/>
                                    <View
                                        android:layout_width="70dp"
                                        android:layout_height="2dp"
                                        android:background="@color/tab_color_press4"
                                        android:layout_gravity="center_vertical"
                                        />
                                    <TextView
                                        android:id="@+id/tv_relationship3"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:text=""
                                        android:textSize="@dimen/text_medium_size"
                                        android:background="@drawable/ll_round_green"
                                        android:padding="@dimen/default_view_margin"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_marginLeft="32dp">
                                    <com.madx.bwm.widget.CircularNetworkImage
                                        android:id="@+id/cni3"
                                        android:layout_width="80dp"
                                        android:layout_height="80dp"
                                        app:border="false"
                                        app:shadow="false"
                                        app:border_width="0dp"
                                        android:src="@drawable/network_image_default"/>
                                    <TextView
                                        android:id="@+id/tv_name3"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginLeft="16dp"
                                        android:text=""
                                        android:singleLine="true"
                                        android:textSize="@dimen/text_large_size"
                                        android:textColor="@android:color/black"
                                        android:gravity="center_vertical"/>
                            </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_4"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="72dp"
                                android:orientation="horizontal">
                                    <View
                                        android:layout_width="2dp"
                                        android:layout_height="50dp"
                                        android:background="@color/tab_color_press4"/>
                                    <View
                                        android:layout_width="70dp"
                                        android:layout_height="2dp"
                                        android:background="@color/tab_color_press4"
                                        android:layout_gravity="center_vertical"
                                        />
                                    <TextView
                                        android:id="@+id/tv_relationship4"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:text=""
                                        android:textSize="@dimen/text_medium_size"
                                        android:background="@drawable/ll_round_green"
                                        android:padding="@dimen/default_view_margin"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_marginLeft="32dp">
                                    <com.madx.bwm.widget.CircularNetworkImage
                                        android:id="@+id/cni4"
                                        android:layout_width="80dp"
                                        android:layout_height="80dp"
                                        app:border="false"
                                        app:shadow="false"
                                        app:border_width="0dp"
                                        android:src="@drawable/network_image_default"/>
                                    <TextView
                                        android:id="@+id/tv_name4"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginLeft="16dp"
                                        android:text=""
                                        android:singleLine="true"
                                        android:textSize="@dimen/text_large_size"
                                        android:textColor="@android:color/black"
                                        android:gravity="center_vertical"/>
                            </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

        </ScrollView>


</LinearLayout>
